{
	"image": "mcr.microsoft.com/devcontainers/base:bullseye",
	"customizations": {
		"vscode": {
			"extensions": [
				"denoland.vscode-deno"
			]
		}
	},
	"postCreateCommand": {
		"install-deno": "curl -fsSL https://deno.land/install.sh | sh -s -- --yes",
		"install-duckdb": "curl https://install.duckdb.org | sh"
	}
}
